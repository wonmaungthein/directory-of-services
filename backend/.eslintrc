{
    "parser": "babel-eslint",
    "extends": "airbnb-base",
    "rules": {
      "indent": ["error", 2],
      "comma-dangle": [
        2,
        "never"
      ],
      "semi": 0,
      "max-len": 0,
      "no-multiple-empty-lines": 0,
      "object-shorthand": 0,
      "padded-blocks": 0
    }
  }