{
  "name": "dos-backend",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "seed": "./node_modules/.bin/knex seed:run --env development",
    "migrate": "./node_modules/.bin/knex migrate:latest --env development",
    "migrate-prod": "./node_modules/.bin/knex migrate:latest --env production",
    "start": "npm run migrate && babel-node ./bin/www  --exec babel-node ",
    "dev": "npm run migrate && nodemon --exec babel-node ./bin/www",
    "start-prod": "git pull; pm2 kill; pm2 start --interpreter babel-node bin/www; pm2 logs",
    "lint": "./node_modules/.bin/eslint **/*.js",
    "fix": "find ./node_modules/* -mtime +10950 -exec touch {} \\;",
    "test": "mocha --timeout 10000 --compilers js:babel-core/register 'test/*.js' --exit"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "dependencies": {
    "babel-plugin-transform-object-assign": "^6.22.0",
    "babel-preset-env": "^1.7.0",
    "babel-register": "^6.26.0",
    "bcrypt": "^2.0.1",
    "body-parser": "~1.18.2",
    "chai": "^4.1.2",
    "compression": "^1.7.2",
    "cors": "^2.8.4",
    "debug": "~2.6.9",
    "dotenv": "^5.0.1",
    "express": "~4.15.5",
    "express-promise-router": "^3.0.1",
    "faker": "^4.1.0",
    "join-js": "^1.0.0",
    "jsonwebtoken": "^8.2.0",
    "knex": "^0.14.4",
    "method-override": "^2.3.10",
    "morgan": "^1.9.0",
    "node-fetch": "^2.1.2",
    "nodemailer": "^4.6.5",
    "nodemailer-mailgun-transport": "^1.4.0",
    "objection": "^1.0.0",
    "passport": "^0.4.0",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "pg": "^7.4.1",
    "randomstring": "^1.1.5",
    "supertest": "^3.1.0"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-eslint": "^8.2.2",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-stage-2": "^6.24.1",
    "eslint": "^4.18.1",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-plugin-babel": "^5.1.0",
    "eslint-plugin-import": "^2.12.0",
    "mocha": "^5.2.0",
    "nodemon": "^1.15.1",
    "pm2": "^2.10.4"
  }
}
